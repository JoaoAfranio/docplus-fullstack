{
  "name": "docplus-back",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migration:run": "prisma migrate deploy",
    "migration:generate": "npm run dev:load-envs prisma migrate dev",
    "seed": "npm run prod:load-envs -- ts-node prisma/seed.ts",
    "prod:load-envs": "dotenv -e .env",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "dev:load-envs": "dotenv -e .env.development",
    "dev:migration:run": "npm run dev:load-envs prisma migrate deploy",
    "dev:migration:generate": "npm run dev:load-envs prisma migrate dev",
    "dev": "cross-env NODE_ENV=development nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register ./src/server.ts' -e ts",
    "dev:seed": "npm run dev:load-envs prisma db seed",
    "prisma:studio": "dotenv -e .env.development npx prisma studio"
  },
  "keywords": [
    "docplus",
    "back-end",
    "typescript",
    "express",
    "nodejs",
    "postgres"
  ],
  "author": "",
  "license": "ISC",
  "prisma": {
    "seed": "npm run dev:load-envs -- ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@brazilian-utils/brazilian-utils": "^1.0.0-rc.12",
    "@prisma/client": "^4.11.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "http-status": "^1.6.2",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.10",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.1.0",
    "nodemon": "^2.0.22",
    "prisma": "^4.11.0",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.8.5",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.2",
    "typescript-transform-paths": "^3.4.6"
  }
}
